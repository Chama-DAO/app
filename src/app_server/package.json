{
  "scripts": {
    "dfx_install": "DFX_VERSION=0.15.1 sh -ci \"$(curl -fsSL https://sdk.dfinity.org/install.sh)\"",
    "replica_start": "dfx start --background",
    "replica_stop": "dfx stop",
    "canister_deploy_local": "dfx deploy app_server",
    "canister_deploy_mainnet": "dfx deploy --network ic app_server",
    "canister_uninstall": "dfx canister uninstall-code app_server",
    "canister_call_get_message": "dfx canister call app_server getMessage",
    "canister_call_set_message": "dfx canister call app_server setMessage '(\"Hello world!\")'"
  },
  "dependencies": {
    "azle": "0.24.1"
  }
}
