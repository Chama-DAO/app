{
  "canisters": {
    "app_backend": {
      "build": "npx azle app_backend",
      "candid": "src/app_backend/app_backend.did",
      "gzip": true,
      "main": "src/app_backend/src/index.ts",
      "tech_stack": {
        "cdk": {
          "azle": {
            "version": "0.24.1"
          }
        },
        "language": {
          "javascript": {},
          "typescript": {}
        }
      },
      "type": "custom",
      "wasm": ".azle/app_backend/app_backend.wasm"
    },
    "app_server": {
      "type": "custom",
      "main": "src/app_server/src/index.ts",
      "candid": "src/app_server/index.did",
      "build": "npx azle app_server",
      "wasm": ".azle/app_server/app_server.wasm",
      "gzip": true,
      "tech_stack": {
        "cdk": {
          "azle": {
            "version": "0.24.1"
          }
        },
        "language": {
          "javascript": {},
          "typescript": {}
        }
      }
    },
    "app_frontend": {
      "dependencies": ["app_server"],
      "source": ["src/app_frontend/dist"],
      "type": "assets",
      "workspace": "app_frontend"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}
